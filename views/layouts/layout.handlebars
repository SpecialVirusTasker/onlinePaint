	  <script type = "text/javascript" src = "/js/cleanMe.js"></script>
	  <script type = "text/javascript" src = "/js/colorMe.js"></script>
	  <script type = "text/javascript" src = "/js/deleteMe.js"></script>
	  <script type = "text/javascript" src = "/js/flipMe.js"></script>
